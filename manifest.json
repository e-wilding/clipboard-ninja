{
  "manifest_version": 2,
  "name": "CopyNinja",
  "version": "1.0",
  "description": "Add multiple items to your clipboard.",
  "content_scripts": [
    {
      "matches": ["http://google.com/*"],
      "js": ["jquery.min.js", "./main.js"],
      "css": ["index.css"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16" : "ninja_icon.png",
      "32" : "ninja_icon.png",
      "64" : "ninja_icon.png",
      "128" : "ninja_icon.png"
    }
  }
}